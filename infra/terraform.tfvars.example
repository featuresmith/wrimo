cloudflare_account_id = "00000000000000000000000000000000"
cloudflare_api_token  = "cf-api-token"
worker_name           = "wrimo"
worker_script = <<-EOT
addEventListener("fetch", event => {
  event.respondWith(new Response("Wrimo Worker placeholder", { status: 200 }));
});
EOT
worker_compatibility_date = "2024-05-01"

worker_secrets = {
  # EXAMPLE_KEY = "value"
}

custom_domains = {
  "wrimo.io" = {
    zone = "wrimo.io"
    # zone_id     = "00000000000000000000000000000000"
    # environment = "production"
  }
}

# Alternatively, set a default zone and provide hostnames as a list:
# custom_domain_default_zone = "wrimo.io"
# custom_domains = [
#   "wrimo.io",
#   # { hostname = "www.wrimo.io", environment = "production" }
# ]
